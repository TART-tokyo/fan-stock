{
    "Comment": "A state machine that generate, upload to IPFS and split as merkle proofs with address list.",
    "StartAt": "Generate Input",
    "States": {
        "Generate Input": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:ap-northeast-1:179855544942:function:merkle-tree-InputGeneratorFunction-QHN6LA3GV43G",
            "Retry": [
                {
                    "ErrorEquals": [
                        "States.TaskFailed"
                    ],
                    "IntervalSeconds": 3,
                    "MaxAttempts": 3,
                    "BackoffRate": 1.5
                }
            ],
            "Next": "Generate"
        },
        "Generate": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:ap-northeast-1:179855544942:function:merkle-tree-GeneratorFunction-1X9XNEPNVHDIG",
            "Retry": [
                {
                    "ErrorEquals": [
                        "States.TaskFailed"
                    ],
                    "IntervalSeconds": 2,
                    "MaxAttempts": 3,
                    "BackoffRate": 1
                }
            ],
            "Next": "Upload"
        },
        "Upload": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:ap-northeast-1:179855544942:function:merkle-tree-UploaderFunction-1E2TG0GVVNPTE",
            "Retry": [
                {
                    "ErrorEquals": [
                        "States.TaskFailed"
                    ],
                    "IntervalSeconds": 2,
                    "MaxAttempts": 3,
                    "BackoffRate": 1
                }
            ],
            "Next": "Split"
        },
        "Split": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:ap-northeast-1:179855544942:function:merkle-tree-SplitterFunction-W2D0UXB1EQBX",
            "Retry": [
                {
                    "ErrorEquals": [
                        "States.TaskFailed"
                    ],
                    "IntervalSeconds": 2,
                    "MaxAttempts": 3,
                    "BackoffRate": 1
                }
            ],
            "End": true
        }
    }
}
